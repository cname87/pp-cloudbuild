  },
  "comments": {
    "scripts": {
      "debug": "Run server allowing VSCode to connect to allow debug - only start from VSCode.",
      "isServerUp": "Pings the server.",
      "checkServer": "Checks if the local server is up & starts it if necessary.  This either finds the server started and exits with code 0 or starts a server in an external terminal and exits with code 0.  Use the Taskkill node.exe' task to kill the server if required.",
      "delDist": "Used before compilation to delete the compiled dist file.",
      "tscBackend": "Compiles backend.",
      "tscBackendWatch": "Compiles backend and recompiles on file change - used during development.",
      "loadSecretsFiles": "When a build is executed from GCP via a github trigger the secret files, and any empty directories, are not there => they are downloaded from GCP Storage.  This script uploads the files.",
      "gitPush": "Uploads secrets to GCP Storage and does a git push to the remote origin - add the branch to be pushed to as a parameter, i.e.'npm run gitPush -- master'. (-u => add tracking reference, -v => verbose)."
    }
  },
  "scripts": {
    "test": "mocha --config ./.mocharc.json ./dist/src/test/testSetup.js ./dist/src/test/*.test.js ./dist/src/**/test/*.test.js",
    "start": "node ./dist/src/app.js",
    "isServerUp": "ts-node --project ./tsconfig.json ./utils-build/isServerUp.ts",
    "checkServer": "ts-node --project ./tsconfig.json ./utils-build/checkServer.ts",
    "delDist": "ts-node ./utils-build/delDistDir.ts",
    "tscBackend": "tsc --project ./tsconfig.json",
    "tscBackendWatch": "tsc --watch --project ./tsconfig.json",
    "compile": "npm-run-all delDist tscBackend",
    "prettierWrite": "prettier --write \"./**/*.ts\" \"./**/*.js\"",
    "eslint:utils": "eslint --ext .js,.ts ./utils-build",
    "eslint:src": "eslint --ext .js,.ts ./src",
    "eslint": "npm run eslint:utils  && npm run eslint:src",
    "loadSecretsFiles": "ts-node ./utils-build/syncGCPStorageUtil.ts",
    "build": "npm-run-all prettierWrite eslint compile loadSecretsFiles",
    "gitPush": "npm run loadSecretsFiles && git push -u -v origin"
  },
